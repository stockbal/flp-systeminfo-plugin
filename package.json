{
    "name": "flp-systeminfo-plugin",
    "version": "0.0.2",
    "description": "Fiori Launchpad Plugin to display Logon information and change client/language",
    "private": true,
    "scripts": {
        "upgrade-packages": "ncu -u",
        "lint": "eslint webapp",
        "build": "ui5 build --include-task=generateManifestBundle generateCachebusterInfo --exclude-task=ui5-task-nwabap-deployer --clean-dest",
        "dev": "ui5 serve --port 1081 --open \"test/flpSandbox.html?sap-language=EN#Shell-home\"",
        "deploy": "ui5 build --include-task=generateManifestBundle generateCachebusterInfo --clean-dest"
    },
    "devDependencies": {
        "@openui5/ts-types": "^1.60.5",
        "dotenv": "^8.2.0",
        "npm-check-updates": "^10.2.3",
        "npm-run-all": "^4.1.5",
        "parseurl": "1.3.3",
        "ui5-middleware-livereload": "^0.4.3",
        "ui5-middleware-livetranspile": "^0.2.4",
        "ui5-middleware-route-proxy": "1.0.7",
        "ui5-middleware-servestatic": "^0.3.3",
        "ui5-task-nwabap-deployer": "^1.0.15",
        "ui5-task-transpile": "^0.2.3"
    },
    "ui5": {
        "dependencies": [
            "ui5-middleware-livereload",
            "ui5-middleware-route-proxy",
            "ui5-middleware-livetranspile",
            "ui5-middleware-servestatic",
            "ui5-task-transpile",
            "ui5-task-nwabap-deployer"
        ]
    }
}
